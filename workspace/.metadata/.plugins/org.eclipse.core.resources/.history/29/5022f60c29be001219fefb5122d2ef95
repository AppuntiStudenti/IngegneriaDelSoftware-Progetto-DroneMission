import it.unibo.is.interfaces.IBasicEnvAwt;

public class DisplayGuiLocation extends Panel implements IGaugeDisplay {
	
	private static final long serialVersionUID = 1L;
	protected IBasicEnvAwt env;
	protected TextField lat;
	protected TextField lon;
	protected String curValToShow = "not yet initialized";
	
	public DisplayGuiLocation(){
		   lat = new TextField(9);
		   lat.setFont(new java.awt.Font("Arial",java.awt.Font.BOLD,24));
		   add(lat);
		   lon = new TextField(9);
		   lon.setFont(new java.awt.Font("Arial",java.awt.Font.BOLD,24));
		   add(lon);
		   env.addPanel(this);
	}
		
